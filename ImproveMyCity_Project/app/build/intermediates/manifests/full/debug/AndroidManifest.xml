<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mk4droid.IMCity_PackDemo"
    android:installLocation="auto"
    android:versionCode="72"
    android:versionName="2.8.1" >

    <uses-sdk
        android:minSdkVersion="10"
        android:targetSdkVersion="10" />

    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.KILL_BACKGROUND_PROCESSES" />
    <uses-permission android:name="android.permission.VIBRATE" />

    <permission
        android:name="com.mk4droid.IMCity_Pack.permission.MAPS_RECEIVE"
        android:protectionLevel="signature" />

    <uses-permission android:name="com.mk4droid.IMCity_Pack.permission.MAPS_RECEIVE" />
    <uses-permission android:name="com.google.android.providers.gsf.permission.READ_GSERVICES" />

    <uses-feature android:name="android.hardware.camera" />
    <uses-feature
        android:glEsVersion="0x00020000"
        android:required="true" />

    <application
        android:icon="@drawable/imc_logo"
        android:label="@string/app_name"
        android:theme="@android:style/Theme.NoTitleBar" >
        <service
            android:name="com.mk4droid.IMC_Services.Service_Data"
            android:enabled="true"
            android:exported="false" />
        <service
            android:name="com.mk4droid.IMC_Services.Service_Location"
            android:enabled="true"
            android:exported="false" />

        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
        <meta-data
            android:name="com.google.android.maps.v2.API_KEY"
            android:value="AIzaSyDbjhEg7ZOvddfpIvFHzUeaDy9-vPA_VBg" />

        <activity
            android:name="com.mk4droid.IMC_Activities.Activity_Thinking"
            android:configChanges="orientation"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.mk4droid.IMC_Activities.FActivity_TabHost"
            android:configChanges="orientation"
            android:theme="@style/PreferencesTheme" />
        <activity
            android:name="com.mk4droid.IMC_Activities.Activity_Splash_Login"
            android:theme="@style/PreferencesTheme" />
        <activity
            android:name="com.mk4droid.IMC_Activities.Activity_Register"
            android:theme="@style/PreferencesTheme" />
        <activity
            android:name="com.mk4droid.IMC_Activities.Activity_Setup"
            android:theme="@style/PreferencesTheme" />
        <activity
            android:name="com.mk4droid.IMC_Activities.Activity_Information_Detailed"
            android:theme="@style/PreferencesTheme" />
    </application>

</manifest>